// Minimal schema so Prisma generates PrismaClient for @hos-marketplace/database-common.
// Services extend BasePrismaService and use their own schema-generated client at runtime.

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Placeholder {
  id String @id @default(uuid())
}
